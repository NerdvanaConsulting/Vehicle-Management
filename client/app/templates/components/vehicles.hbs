<article class="vehicle">
<div width="100%">
<table style="border: 1px solid black" align="center" width="90%">
   <thead>
      <tr>
         <th colspan="2">Vehicle Details</th>
      </tr>
   </thead>
   <tbody>
      <tr>
         <td>Year</td>
         <td>{{vehyear}}</td>
      </tr>
      <tr>
         <td>Make</td>
         <td>{{vehmake}}</td>
      </tr>
      <tr>
         <td>Model</td>
         <td>{{vehmodel}}</td>
      </tr>
      <tr>
         <td>Color</td>
         <td>{{vehcolor}}</td>
      </tr>
      <tr>
         <td>Mileage</td>
         <td>{{vehmileage}}</td>
      </tr>
      <tr>
         <td colspan="2"><img src={{vehimage}} height=64 alt="{{id}}"></td>
      </tr>
   </tbody>
</table>
</div>
</article>