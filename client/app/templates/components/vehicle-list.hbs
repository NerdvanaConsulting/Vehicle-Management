<article class="vehicle-list">
<div width="100%">
<table style="border: 1px solid black" align="center" width="90%">
   <thead>
      <tr>
         <th colspan="3">{{vehyear}} {{vehmake}} {{vehmodel}}</th>
      </tr>
   </thead>
   <tbody>
      <tr>
         <td rowspan="2"><img src={{vehimage}} height=64 alt=""></td>
         <td rowspan="1">Color</td><td>Mileage</td>
      </tr>
      <tr>
         <td>{{vehcolor}}</td><td>{{vehmileage}}</td>
      </tr>
   </tbody>
</table>
</div>
</article>